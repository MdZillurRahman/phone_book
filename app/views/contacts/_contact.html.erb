
<tr id="<%= dom_id contact %>">
  <td>
    <%= contact.first_name %>
  </td>

  <td>
    <%= contact.last_name %>
  </td>

  <td>
    <%= contact.email %>
  </td>

  <td>
    <%= contact.phone %>
  </td>

  <td>
    <%= contact.twitter %>
  </td>

  <td class="d-flex gap-3">
    <%= link_to "Show", contact %>
    <%= button_to "Destroy", contact, method: :delete, data: { confirm: 'Are you sure?' } %>
  </td> 

</tr>
